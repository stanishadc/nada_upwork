@model DashBoardStatistic
@{
    ViewData["Title"] = "Project Details";
    Layout = "_PublicLayout";
}
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0">Projects</h4>
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><Link>Home</li>
                    <li class="breadcrumb-item active">Project Details</li>
                </ol>
            </div>
        </div>
    </div>
</div>
@if (TempData["errorMessage"] != null)
{
    <div class="alert alert-danger alert-dismissible fade-show">
        @TempData["errorMessage"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
}
@if (TempData["successMessage"] != null)
{
    <div class="alert alert-success alert-dismissible fade-show">
        @TempData["successMessage"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
}
<div class="alert alert-success">
    <form asp-action="Details">
        <div class="row">
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="projectNumber" class="form-label">ProjectNumber : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ProjectNumber)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="projectName" class="form-label">Project Name : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ProjectName)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="zoneId" class="form-label">Zone : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ZoneName)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="projectCategory" class="form-label">Project Category : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ProjectCategory)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="year" class="form-label">Year : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Year)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="financeEntity" class="form-label">FinanceEntity : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.FinanceEntity)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="contractor" class="form-label">Contractor : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Contractor)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="consultant" class="form-label">Consultant : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Consultant)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="progress" class="form-label">Progress : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Progress)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="projectPeriod" class="form-label">ProjectPeriod : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ProjectPeriod)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="startDateContract" class="form-label">StartDateContract : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.StartDateContract)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="endDateContract" class="form-label">EndDateContract : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.EndDateContract)</label>
                </div>
            </div>
            <div class="col-lg-3">
