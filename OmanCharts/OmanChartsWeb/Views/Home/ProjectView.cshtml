                    <label htmlFor="completionDate" class="form-label">CompletionDate : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.CompletionDate)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="timeExtension" class="form-label">TimeExtension : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.TimeExtension)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="cost" class="form-label">Cost : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Cost)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="identifiedCost" class="form-label">IdentifiedCost : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.IdentifiedCost)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="expenses" class="form-label">Expenses : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Expenses)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="remainingCost" class="form-label">RemainingCost : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.RemainingCost)</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="changeRequestCost" class="form-label">ChangeRequestCost : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.ChangeRequestCost)</label>
                </div>
            </div>        
            <div class="col-lg-3">
                <div class="mb-3">
                    <label htmlFor="requiredAction" class="form-label">RequiredAction : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.RequiredAction)</label>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="mb-3">
                    <label htmlFor="challenges" class="form-label">Challenges : </label>
                    <label htmlFor="zoneName" class="form-label">@Html.DisplayFor(model => model.ProjectData.Challenges)</label>
                </div>
            </div>
        </div>
        <div>
            <a class="btn btn-danger" asp-action="Project">Back</a>
        </div>
    </form>
</div>
